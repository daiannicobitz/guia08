package frsf.isi.died.guia08.problema01.excepciones;

public class ExcepcionNoExisteTareaAsignada extends Exception {
	public ExcepcionNoExisteTareaAsignada() {
		this("No existe esta tarea en la lista de tareas asignadas.");
	}
	
	public ExcepcionNoExisteTareaAsignada(String texto) {
		super("La tarea indicada es incorrecta. "+texto);
	}
}
